<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls>	
	    
    <control Style="BasicHomeFade">
      <description>Feed items</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>190</posX>
      <posY>1000</posY>
      <width>1600</width>
      <align>left</align>
      <font>font11</font>
      <textcolor>ff888888</textcolor>
