          <unfocusedAlpha>160</unfocusedAlpha>
          <folderPrefix />
          <folderSuffix />
          <onleft>2</onleft>
          <onright>2</onright>
        </control>
                  <control>
                    <description>Thumbnail Panel</description>
                    <type>thumbnailpanel</type>
                    <id>50</id>
					<posX>144</posX>
                    <posY>240</posY>
                    <width>1250</width>
                    <height>750</height>
					<onleft>2</onleft>
					<onright>2</onright>				
					<textureUp></textureUp>
					<textureDown></textureDown>
					<textXOff>0</textXOff>
					<textYOff>2250</textYOff>
					<!-- Small Icons 0.66 aspect ratio -->
					<itemWidth>133</itemWidth>
				    <itemHeight>191</itemHeight>
				    <textureWidth>131</textureWidth>
				    <textureHeight>189</textureHeight>
				    <thumbWidth>132</thumbWidth>
				    <thumbHeight>189</thumbHeight>
				    <thumbPosX>0</thumbPosX>
				    <thumbPosY>0</thumbPosY>				
					<!-- Large Icons 0.66 aspect ratio -->
					<textureWidthBig>196</textureWidthBig>
					<textureHeightBig>287</textureHeightBig>
					<itemWidthBig>198</itemWidthBig>
					<itemHeightBig>291</itemHeightBig>
					<thumbWidthBig>195</thumbWidthBig>
					<thumbHeightBig>286</thumbHeightBig>
					<thumbPosXBig>0</thumbPosXBig>
					<thumbPosYBig>0</thumbPosYBig>
					<font>font11</font>
					<suffix>|</suffix>					
					<enableFocusZoom>no</enableFocusZoom>
					<spinWidth>45</spinWidth>
					<spinHeight>45</spinHeight>
