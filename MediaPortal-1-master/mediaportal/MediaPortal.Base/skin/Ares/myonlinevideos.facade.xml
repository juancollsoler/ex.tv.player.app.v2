<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!Control.HasFocus(50)</visible>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
				<control>
					<description>Movie List</description>
					<type>listcontrol</type>
					<id>50</id>
					<font>font12</font>
					<onleft>2</onleft>
					<onright>2</onright>
					<width>1387</width>
					<posX>57</posX>
					<posY>225</posY>
					<height>620</height>
					<textureFocus>btn_list.png</textureFocus>
					<textXOff>-15</textXOff>
					<textYOff>3</textYOff>
					<textYOff2>3</textYOff2>
					<textXOff2>1370</textXOff2>
					<textXOff3>15</textXOff3>
					<IconXOff>3000</IconXOff> 
                    <textureHeight>49</textureHeight>					
					<spinWidth>45</spinWidth>
					<spinHeight>42</spinHeight>
					<spinPosX>2760</spinPosX>
					<spinPosY>380</spinPosY>
					<spinAlign>right</spinAlign>
					<spinColor>99ffffff</spinColor>
					<spinCanFocus>no</spinCanFocus>
					<!-- group headers -->
					<textcolor>ffffffff</textcolor>
					<textcolorNoFocus>ff888888</textcolorNoFocus>
					<playedColor>FFB2D4F5</playedColor>
					<textcolor2>ff666666</textcolor2>
					<textcolor3>ffffffff</textcolor3>
				</control>
				
				
                <control>
                    <description>Thumbnail Panel</description>
                    <type>thumbnailpanel</type>
                    <id>50</id>
					<posX>70</posX>
                    <posY>210</posY>
                    <width>1400</width>
                    <height>620</height>
					<onleft>2</onleft>
					<onright>50</onright>       
					<imageFolder>-</imageFolder>          
					<imageFolderFocus>-</imageFolderFocus>
					<showFrame>true</showFrame>
					<dimColor>99ffffff</dimColor>
					<enableFocusZoom>no</enableFocusZoom>
					<hideUnfocusTexture>no</hideUnfocusTexture>
					<renderFocusText>no</renderFocusText>
					<renderUnfocusText>no</renderUnfocusText>
					<thumbZoom>no</thumbZoom>
					<keepaspectratio>yes</keepaspectratio>
					<frameNoFocus>OnlineVideos\thumbbg3.png</frameNoFocus>
					<frameFocus>onlinevideos_thumb_focus.png</frameFocus>
					<textureMask></textureMask>
					<imageFolderFocus>OnlineVideos\thumbbg3.png</imageFolderFocus>
                    <!-- Small Icons -->
                    <itemWidth>185</itemWidth>
                    <itemHeight>144</itemHeight>
                    <textureWidth>185</textureWidth>
                    <textureHeight>144</textureHeight>
                    <thumbWidth>179</thumbWidth>
                    <thumbHeight>138</thumbHeight>
                    <thumbPosX>3</thumbPosX>
                    <thumbPosY>3</thumbPosY>
                    <!-- Large Icons -->
                    <itemWidthBig>263</itemWidthBig>
                    <itemHeightBig>194</itemHeightBig>
                    <textureWidthBig>263</textureWidthBig>
                    <textureHeightBig>194</textureHeightBig>
                    <thumbWidthBig>249</thumbWidthBig>
                    <thumbHeightBig>184</thumbHeightBig>
                    <thumbPosXBig>7</thumbPosXBig>
                    <thumbPosYBig>5</thumbPosYBig>
					<zoomXPixels>11</zoomXPixels>
					<zoomYPixels>5</zoomYPixels>
					<spinPosX>7828</spinPosX>
					<spinPosY>1000</spinPosY>
					<unfocusedAlpha>110</unfocusedAlpha>  
                 </control>        
      </control>
    </control>
  </controls>
</window>